	<queryString>
		<![CDATA[SELECT count (a.id) TotCat,
  d.display_value  Category,
  ((count (a.id)::float)/
 ( SELECT count (*) 
FROM 
  public.application a1, 
  public.party b1, 
  public.service c1, 
  public.request_type d1
WHERE 
  a1.agent_id = b1.id AND
  c1.application_id = a1.id AND
  c1.request_type_code = d1.code
  AND cast( to_char(a1.lodging_datetime, 'yyyymmdd') as  int) between 20110501 and 20110531
 ) *100 )::numeric(4,2) Perc,
( (count (a.id)::float)/
 (20110531-20110501)):: numeric(4,2) Avg,
(count(a.id)-1) TotReq,
(((count (a.id)-1)::float)/
(count (a.id))*100)::numeric(4,2) PercReq
FROM 
  public.application a, 
  public.party b, 
  public.service c, 
  public.request_type d
WHERE 
  a.agent_id = b.id AND
  c.application_id = a.id AND
  c.request_type_code = d.code
  AND cast( to_char(a.lodging_datetime, 'yyyymmdd') as  int) between 20110501 and 20110531
group by 
  d.display_value;]]>
	</queryString>
